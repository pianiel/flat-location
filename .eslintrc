{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "classes": true,
    "blockBindings": true,
    "destructuring": true,
    "arrowFunctions": true,
    "defaultParams": true,
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
